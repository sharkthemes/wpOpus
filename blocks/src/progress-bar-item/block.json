{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wpopus/progress-bar-item",
	"version": "1.0.0",
	"title": "Progress Bar Item",
	"category": "wpopus",
	"icon": "info-outline",
	"description": "Progress Bar Item",
	"parent": "wpopus/progress-bars",
	"example": {},
	"supports": {
		"html": false,
		"inserter": false
	},
	"attributes": {
		"progressValue": {
			"type": "number",
			"default": 80                                                                                                  
		},
		"progressHeight": {
			"type": "number",
			"default": 5                                                                                                  
		},
		"progressColor": {
			"type": "string",
			"default": "#ff5f54"
		},
		"progressBaseColor": {
			"type": "string",
			"default": "#dddddd"
		}
	},
	"textdomain": "wpopus",
	"style": "file:./style-index.css",
	"editorScript": "file:./index.js"
}